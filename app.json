{
  "expo": {
    "name": "EroSoftTask",
    "slug": "EroSoftTask",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#379734"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.erosoft.taskerosoft",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptiveIcon.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
